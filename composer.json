{
  "name": "askvortsov/pipetables",
  "description": "Active markdown tables.",
  "keywords": [
    "flarum"
  ],
  "type": "flarum-extension",
  "license": "MIT",
  "require": {
    "flarum/core": "^0.1.0-beta.15",
    "flarum/markdown": "^0.1.0-beta.15"
  },
  "authors": [
    {
      "name": "Askvortsov",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      " \\PipeTables\\": "src/"
    }
  },
  "extra": {
    "flarum-extension": {
      "title": "Markdown fas fa-table",
      "icon": {
        "name": "Tables",
        "backgroundColor": "#6932D1",
        "color": "#ffffff"
      }
    }
  }
}
